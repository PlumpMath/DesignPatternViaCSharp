leg:level id="UCPN.CPA.CPAN05.BODY"><leg:level-vrnt leveltype="act" searchtype="LEGISLATION ACT"><leg:levelbody><leg:level id="UCPN.CPA.CPAN05.PT8"><leg:level-vrnt leveltype="part" searchtype="LEGISLATION PART" subdoc="false" toc-caption="PART 8  ENFORCEMENT OF JUDGMENTS AND ORDERS [ss 102&#x2013;138]"><leg:levelbody>
