<leg:level id="UCPN.UCPR.UCPRN05.PT43.D1"><leg:level-vrnt leveltype="division" searchtype="LEGISLATION DIVISION" subdoc="false" toc-caption="DIVISION 1  PRELIMINARY [r 43.1]">
