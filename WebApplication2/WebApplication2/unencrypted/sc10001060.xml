<leg:levelinfo>
<leg:levelstatus isrepealed="false" ln.user-displayed="false"/></leg:levelinfo>
<leg:heading inline="true"><edpnum><refpt id="UCPN.SC.SCA70.S96" type="ext"/>[SCA s 96]</edpnum><title searchtype="SECT-TITLE">Effect of judgment or order</title><desig searchtype="SECT-NUM"><designum>96</designum></desig></leg:heading>
<leg:levelbody>
<leg:level id="UCPN.SC.SCA70.S96.1"><leg:level-vrnt leveltype="subsect" searchtype="LEGISLATION SUBSECT">
<leg:levelinfo>
<leg:levelstatus isrepealed="false" ln.user-displayed="false"/></leg:levelinfo>
<leg:heading inline="true"><desig><designum><refpt id="UCPN.SC.SCA70.S96.1" type="ext"/>(1)</designum></desig></leg:heading>
<leg:levelbody>
<leg:bodytext>
<p><text>Any judgment or order of the Court for the payment of money shall have the effect of a judgment at law.</text></p>
<glp:note>
<p><text><ci:cite searchtype="LEG-REF" type="legref"><ci:content>Act 8 of 1901 s 3(1)</ci:content></ci:cite></text></p></glp:note>
</leg:bodytext>
</leg:levelbody>
</leg:level-vrnt>
</leg:level>
</leg:levelbody>
</leg:level-vrnt>
</leg:level>
