<leg:level id="UCPN.UCPR.UCPRN05.PT27"><leg:level-vrnt leveltype="part" searchtype="LEGISLATION PART" subdoc="false" toc-caption="PART 27 DISPOSAL OF LAND [rr 27.1&#x2013;27.4]">
<leg:levelinfo>
