<leg:level id="UCPN.DDT.DDTRN07.PT1"><leg:level-vrnt leveltype="part" searchtype="LEGISLATION PART" subdoc="false" toc-caption="PART 1 PRELIMINARY [cll 1&#x2013;3]">
