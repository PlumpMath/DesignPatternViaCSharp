<leg:level id="UCPN.DDT.DDTRN07.REG1"><leg:level-vrnt leveltype="reg" searchtype="LEGISLATION REG" subdoc="true" toc-caption="[472,000] reg 1 Name of Regulation">
<leg:levelinfo>
<leg:levelstatus isrepealed="false" ln.user-displayed="false"/></leg:levelinfo>
<leg:heading inline="true"><edpnum><refpt id="UCPN.DDT.DDTRN07.REG1" type="ext"/>[472,000]</edpnum><title searchtype="REG-TITLE">Name of Regulation</title><desig searchtype="REG-NUM"><designum>1</designum></desig></leg:heading>
