<leg:level id="UCPN.SC.SCA70.SCH4.PT1"><leg:level-vrnt leveltype="part" searchtype="LEGISLATION PART" subdoc="false" toc-caption="PART 1 GENERAL [s 1]">
<leg:levelinfo>
