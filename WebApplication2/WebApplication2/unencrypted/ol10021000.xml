<leg:levelbody>
<leg:level id="UCPN.OL.ADR88.R1"><leg:level-vrnt leveltype="rul" searchtype="LEGISLATION RUL" subdoc="true" toc-caption="[255,000] r 1 Name of Rules">
<leg:levelinfo>
<leg:levelstatus isrepealed="false" ln.user-displayed="false"/></leg:levelinfo>
