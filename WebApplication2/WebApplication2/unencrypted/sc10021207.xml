<leg:levelstatus isrepealed="false" ln.user-displayed="false"/></leg:levelinfo>
<leg:heading inline="true"><edpnum><refpt id="UCPN.SC.SCR70.PT79.R1" type="ext"/>[SCR Pt 79 r 1]</edpnum><title searchtype="RUL-TITLE">Application of Part</title><desig searchtype="RUL-NUM"><designum>1</designum></desig></leg:heading>
