<ci:cite searchtype="LEG-REF"><ci:content><remotelink dpsi="05CC" refpt="OTH.PEEN.S164" remotekey1="REFPTID" service="DOC-ID" status="unval">s 164</remotelink></ci:content></ci:cite>
: <ci:cite searchtype="CASE-REF"><ci:case><ci:caseinfo><ci:decisiondate year="1904"/></ci:caseinfo><ci:caseref ID="cr003124" spanref="cr003124-001" status="unval"><ci:reporter value="CLR"/><ci:volume num="1"/><ci:page num="39"/></ci:caseref></ci:case><ci:content><citefragment searchtype="CASE-NAME-REF"><emph typestyle="it">Chanter v Blackwood (No 1)</emph></citefragment><citefragment searchtype="CASE-CITE-REF"> <ci:span spanid="cr003124-001" status="unval">(1904) 1 CLR 39</ci:span></citefragment> <remotelink dpsi="0018" refpt="9812" remotekey1="REFPTID" service="DOC-ID"><inlineobject alttext="Click for CaseBase entry" attachment="web-server" filename="cbcc.gif" type="image"/></remotelink></ci:content></ci:cite>; <ci:cite searchtype="CASE-REF"><ci:case><ci:caseinfo><ci:decisiondate year="1980"/></ci:caseinfo><ci:caseref ID="cr003125" spanref="cr003125-001" status="unval"><ci:reporter value="SASR"/><ci:volume num="23"/><ci:page num="321"/></ci:caseref></ci:case><ci:content><citefragment searchtype="CASE-NAME-REF"><emph typestyle="it">Crafter v Webster (No 2)</emph></citefragment><citefragment searchtype="CASE-CITE-REF"> <ci:span spanid="cr003125-001" status="unval">(1980) 23 SASR 321</ci:span></citefragment> <remotelink dpsi="0018" refpt="3283" remotekey1="REFPTID" service="DOC-ID"><inlineobject alttext="Click for CaseBase entry" attachment="web-server" filename="cbcc.gif" type="image"/></remotelink></ci:content></ci:cite>; <ci:cite searchtype="CASE-REF"><ci:case><ci:caseinfo><ci:decisiondate year="1980"/></ci:caseinfo><ci:caseref ID="cr003126" spanref="cr003126-001" status="unval"><ci:reporter value="SASR"/><ci:volume num="26"/><ci:page num="129"/></ci:caseref></ci:case><ci:content><citefragment searchtype="CASE-NAME-REF"><emph typestyle="it">Beck v Porter</emph></citefragment><citefragment searchtype="CASE-CITE-REF"> <ci:span spanid="cr003126-001" status="unval">(1980) 26 SASR 129</ci:span></citefragment> <remotelink dpsi="0018" refpt="119825" remotekey1="REFPTID" s