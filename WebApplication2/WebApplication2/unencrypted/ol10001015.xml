<leg:level id="UCPN.OL.ADA88.PT6"><leg:level-vrnt leveltype="part" searchtype="LEGISLATION PART" subdoc="false" toc-caption="PART VI REPEALS AND AMENDMENTS OF IMPERIAL LEGISLATION [ss 44&#x2013;47]">
<leg:levelinfo>
