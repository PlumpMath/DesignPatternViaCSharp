<tgroup cols="10" colsep="0" rowsep="0">
<colspec colname="col1" colnum="1" colwidth="54*"/>
<colspec colname="col2" colnum="2" colwidth="325*"/>
<colspec colname="col3" colnum="3" colwidth="190*"/>
<colspec colname="col4" colnum="4" colwidth="278*"/>
