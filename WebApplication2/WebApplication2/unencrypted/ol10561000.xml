<leg:levelbody>
<leg:level id="UCPN.OL.LPRN05.REG1"><leg:level-vrnt leveltype="reg" searchtype="LEGISLATION REG" subdoc="true" toc-caption="[380,000] reg 1 Name of Regulation">
<leg:levelinfo>
