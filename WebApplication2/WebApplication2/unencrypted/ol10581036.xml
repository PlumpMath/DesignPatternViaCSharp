elbody>
<leg:level id="UCPN.OL.LAN.PT4.D3"><leg:level-vrnt leveltype="division" searchtype="LEGISLATION DIVISION" subdoc="false" toc-caption="DIVISION 3 GENERAL [ss 77, 78]">
