<leg:levelinfo>
<leg:levelstatus isrepealed="false" ln.user-displayed="false"/></leg:levelinfo>
<leg:heading><desig searchtype="PART-NUM"><designum><refpt id="UCPN.UCPR.UCPRN05.PT38" type="ext"/>PART 38</designum></desig><title searchtype="PART-TITLE">EXAMINATION OF JUDGMENT DEBTOR [rr 38.1&#x2013;38.7]</title></leg:heading>
<leg:levelbody>
<leg:bodytext>
<p>
<table frame="all" pgwide="1">
<tgroup align="left" cols="5" colsep="0" rowsep="0">
<colspec colname="col1" colsep="0" colwidth="31*" rowsep="0"/>
