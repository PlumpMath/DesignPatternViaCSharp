<row><entry nameend="col3" namest="col1" valign="top">  <emph typestyle="bf">COURT DETAILS</emph>
 </entry></row>
<row><entry nameend="col2" namest="col1" valign="top">  Court
 </entry><entry colname="col2" valign="top">   
 </entry></row>
